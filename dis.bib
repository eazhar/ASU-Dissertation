	langid = {english},
	keywords = {Characterization and Evaluation of Materials, Condensed Matter Physics, Nanotechnology, Operating Procedures, Materials Treatment, Optical and Electronic Materials, Surfaces and Interfaces, Thin Films},
	file = {Li et al - 2011 - Structure and electrical properties of p-type twin ZnTe nanowires.pdf:C\:\\Users\\eazharViz\\Dropbox\\Research\\_Literature Backup\\ZnTe Nanowire\\Li et al - 2011 - Structure and electrical properties of p-type twin ZnTe nanowires.pdf:application/pdf;Snapshot:C\:\\Users\\eazharViz\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\7rz610p7.default\\zotero\\storage\\RDSDBP6Z\\10.html:text/html}
