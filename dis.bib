@article{herrera_rocher_ag_2015,
	title = {Ag {NWs} for {Organic} {Photovoltaic} {Cells}: {Semitransparent} and {Stretchable} {Devices}},
	shorttitle = {Ag {NWs} for {Organic} {Photovoltaic} {Cells}},
	abstract = {The current developments on organic photovoltaic (OPV) solar cells are inspired by the idea that they can be processed from solution. One of the challenges in developing fully solution-processed OPV devices is the design of well-performing electrodes with low cost of deposition. This has been one of the focus of the academic and industrial community. Nowadays, the use of ITO as a semitransparent electrode has become a bottleneck. This is because it is brittle, has Indium (a rare element), and needs a high energy intensive deposition process. There are many alternatives presented but none of them match the performance of ITO. Here, we report a full solution-processed ITO-free semitransparent OPV device based on silver nanowires (Ag NWs). To demonstrate the potential of Ag NWs in OPV devices, they were employed as a bottom and top electrode without a post treatment to decrease their roughness. Record devices achieved only a decrease of 6.5\% of efficiency in comparison to ITO based reference devices. Furthermore, due to its good bending properties, a working stretchable device was fabricated by depositing the different OPV layers on a pre-stretched elastomeric membrane. These results indicate that Ag NWs can successfully replace ITO as an electrode.},
	urldate = {2017-01-03},
	author = {Herrera Rocher, F. R.},
	year = {2015},
	file = {Herrera Rocher - 2015 - Ag NWs for Organic Photovoltaic Cells.pdf:/Users/eazhar/Dropbox/Research/_Literature Backup//Electrochromic/Polymer Solar/stretchable/Herrera Rocher - 2015 - Ag NWs for Organic Photovoltaic Cells.pdf:application/pdf;Snapshot:/Users/eazhar/Library/Application Support/Firefox/Profiles/vv3y72ot.default-1470452355141/zotero/storage/NKRR4NP8/uuida4dd7506-88be-42d6-8a82-fc95e57095c1.html:text/html}
}

