@article{winther-jensen_new_2015,
	title = {New one-pot poly(3,4-ethylenedioxythiophene): poly(tetrahydrofuran) memory material for facile fabrication of memory organic electrochemical transistors},
	volume = {3},
	issn = {2166-532X},
	shorttitle = {New one-pot poly(3,4-ethylenedioxythiophene)},
