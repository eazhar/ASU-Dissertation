@article{strelcov_deep_2014,
	title = {Deep {Data} {Analysis} of {Conductive} {Phenomena} on {Complex} {Oxide} {Interfaces}: {Physics} from {Data} {Mining}},
	volume = {8},
	issn = {1936-0851},
	shorttitle = {Deep {Data} {Analysis} of {Conductive} {Phenomena} on {Complex} {Oxide} {Interfaces}},
	doi = {10.1021/nn502029b},
	url = {http://doi.org/10.1021/nn502029b},
	abstract = {Spatial variability of electronic transport in BiFeO3–CoFe2O4 (BFO–CFO) self-assembled heterostructures is explored using spatially resolved first-order reversal curve (FORC) current voltage (IV) mapping. Multivariate statistical analysis of FORC-IV data classifies statistically significant behaviors and maps characteristic responses spatially. In particular, regions of grain, matrix, and grain boundary responses are clearly identified. k-Means and Bayesian demixing analysis suggest the characteristic response be separated into four components, with hysteretic-type behavior localized at the BFO–CFO tubular interfaces. The conditions under which Bayesian components allow direct physical interpretation are explored, and transport mechanisms at the grain boundaries and individual phases are analyzed. This approach conjoins multivariate statistical analysis with physics-based interpretation, actualizing a robust, universal, data-driven approach to problem solving, which can be applied to exploration of local transport and other functional phenomena in other spatially inhomogeneous systems.},
	number = {6},
	urldate = {2016-08-19},
	journal = {ACS Nano},
	author = {Strelcov, Evgheni and Belianinov, Alexei and Hsieh, Ying-Hui and Jesse, Stephen and Baddorf, Arthur P. and Chu, Ying-Hao and Kalinin, Sergei V.},
