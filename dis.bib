	month = sep,
	year = {2014},
	keywords = {Charge recombination, Electronic structures, Interfacial charge transfer, Photocatalytic activity, Surface doping},
	pages = {466--475},
	file = {Chang_Liu-2014-The roles of surface-doped metal ions (V, Mn, Fe, Cu, Ce, and W) in the.pdf:/Users/eazhar/Dropbox/Research/_Literature Backup//Electrochromic/W-TiO2/Chang_Liu-2014-The roles of surface-doped metal ions (V, Mn, Fe, Cu, Ce, and W) in the.pdf:application/pdf;ScienceDirect Snapshot:/Users/eazhar/Library/Application Support/Firefox/Profiles/vv3y72ot.default-1470452355141/zotero/storage/KJ3E6CEE/S0926337314002094.html:text/html}
}

@article{costa_electrochromic_2012,
