	note = {00030},
	keywords = {Anisotropy, II-VI semiconductors, Multiple quantum wells, nanowires, Surface morphology},
	pages = {094309}
}

@article{zhang_structural_2011,
	title = {Structural phase transition and the related electronic and optical properties of {MgZnO} nanowires},
