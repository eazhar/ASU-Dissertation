	keywords = {Electrochromic, Nanospheres, Photovoltaic, polyaniline, Polyanionic poly(sodium 4-styrenesulfonate), Silicon thin film solar cell},
	pages = {69--75},
	file = {Huang et al - 2016 - Integration of polyelectrolyte based electrochromic material in printable.pdf:/Users/eazhar/Dropbox/Research/_Literature Backup//Electrochromic/Self-Powered/Huang et al - 2016 - Integration of polyelectrolyte based electrochromic material in printable.pdf:application/pdf;ScienceDirect Snapshot:/Users/eazhar/Library/Application Support/Firefox/Profiles/vv3y72ot.default-1470452355141/zotero/storage/DHU2BP5T/S0927024815002160.html:text/html}
