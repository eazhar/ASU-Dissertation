	keywords = {electrochromic devices, Inkjet printing, Poly(3,4-ethylenedioxythiophene):polystyrene sulphonate, Tungsten oxide, Wet ball-milling},
	pages = {276--282},
	file = {Nguyen et al-2016-One-step inkjet printing of tungsten oxide-poly(3,4-ethylenedioxythiophene).pdf:/Users/eazhar/Dropbox/Research/_Literature Backup//Electrochromic/Inkjet/2016/Nguyen et al-2016-One-step inkjet printing of tungsten oxide-poly(3,4-ethylenedioxythiophene).pdf:application/pdf;ScienceDirect Snapshot:/Users/eazhar/Library/Application Support/Firefox/Profiles/vv3y72ot.default-1470452355141/zotero/storage/KKJB6AW2/S0040609016001292.html:text/html}
