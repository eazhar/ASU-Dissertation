@article{torres-huerta_effect_2011,
	title = {Effect of the substrate on the properties of {ZnO}–{MgO} thin films grown by atmospheric pressure metal-organic chemical vapor deposition},
	volume = {519},
	issn = {0040-6090},
	doi = {10.1016/j.tsf.2011.03.030},
	url = {http://doi.org/10.1016/j.tsf.2011.03.030},
	abstract = {The ZnO–MgO alloys possess attractive properties for possible applications in optoelectronic and display devices; however, the optical properties are strongly dependent on the deposition parameters. In this work, the effect of the glassy and metallic substrates on the structural, morphological and optical properties of ZnO–MgO thin films using atmospheric pressure metal-organic chemical vapor deposition was investigated at relatively low deposition temperature, 500 °C. Magnesium and zinc acetylacetonates were used as the metal-organic source. X-ray diffraction experiments provided evidence that the kind of substrates cause a deviation of c-axis lattice constant due to the constitution of a oxide mixture (ZnO and MgO) in combination with different intermetallic compounds(Mg2Zn11 and Mg4Zn7) in the growth films. The substitutional and interstitial sites of Mg2+ instead of Zn2+ ions in the lattice are the most probable mechanism to form intermetallic compounds. The optical parameters as well as thickness of the films were calculated by Spectroscopic Ellipsometry using the classical dispersion model based on the sum of the single and double Lorentz and Drude oscillators in combination with Kato–Adachi equations, as well as X-ray reflectivity.},
	number = {18},
	urldate = {2014-11-28},
	journal = {Thin Solid Films},
	author = {Torres-Huerta, A. M. and Domínguez-Crespo, M. A. and Brachetti-Sibaja, S. B. and Arenas-Alatorre, J. and Rodríguez-Pulido, A.},
	month = jul,
	year = {2011},
	note = {00004},
	keywords = {Atmospheric pressure metalorganic chemical vapor deposition, Ellipsometry, Magnesium oxide, Optical properties, Surface morphology, thin films, Zinc oxide},
	pages = {6044--6052}
}

