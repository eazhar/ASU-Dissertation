	file = {IOP Full Text PDF:/Users/eazhar/Library/Application Support/Firefox/Profiles/vv3y72ot.default-1470452355141/zotero/storage/I7784HN3/Deepa et al. - 2006 - A case study of optical properties and structure o.pdf:application/pdf}
}

