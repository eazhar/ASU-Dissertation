@article{yakuphanoglu_controlling_2011,
	title = {Controlling of electrical and interface state density properties of {ZnO}:{Co}/p-silicon diode structures by compositional fraction of cobalt dopant},
	volume = {51},
	issn = {0026-2714},
	shorttitle = {Controlling of electrical and interface state density properties of {ZnO}},
	doi = {10.1016/j.microrel.2011.05.013},
	url = {http://doi.org/10.1016/j.microrel.2011.05.013},
	abstract = {The heterojunction diodes based on cobalt doped zinc oxide (ZnO) were prepared by sol–gel deposition method. The compositional fraction of cobalt dopant was varied to control the electrical parameters of the diode. Atomic force microscopy was used to determine the structural properties of ZnO:Co films. The ZnO:Co films have a microfiber structure and the structure of microfibers was changed with the cobalt dopant. The ideality factor values of 5\% and 15\% Co doped ZnO:Co/p-Si diodes were determined to be 3.49 and 7.51, respectively. The barrier height of the ZnO:Co/p-Si diodes were found to vary from 0.75eV to 0.78eV. It is concluded that the electrical and interface state density properties of ZnO:Co/p-Si diodes can be controlled by compositional fraction of cobalt dopant.},
	number = {12},
	urldate = {2017-05-30},
	journal = {Microelectronics Reliability},
	author = {Yakuphanoglu, F.},
	month = dec,
	year = {2011},
	pages = {2195--2199},
	file = {ScienceDirect Snapshot:/Users/eazhar/Library/Application Support/Firefox/Profiles/3lz1vxss.default-1491192058467/zotero/storage/QF2KXTKP/S0026271411001788.html:text/html}
}

@article{aydogan_electrical_2009,
	title = {Electrical characterization of {Au}/n-{ZnO} {Schottky} contacts on n-{Si}},
	volume = {476},
	issn = {0925-8388},
	doi = {10.1016/j.jallcom.2008.09.131},
	url = {http://doi.org/10.1016/j.jallcom.2008.09.131},
	abstract = {Wide band gap semiconducting layer of n-type ZnO thin film was fabricated on n-type Si substrate with electrochemical deposition technique and the current–voltage (I–V) and the capacitance–voltage/frequency (C–V/f) characteristics of the structure have been measured at room temperature. The characteristic parameters of the structure such as barrier height, ideality factor and series resistance were determined from the current–voltage measurements. Also, Cheung functions and Norde method were used to plot the I–V characteristics and, to extract the characteristic parameters of the Schottky contact. It was seen that trap-filled space charge-limited current (SCLC) is the dominant transport mechanism at large forward bias. The capacitance measurements showed that the values of capacitance were almost independent of frequency up to a certain value of frequency whereas at high frequencies the capacitance decreased quickly. The higher values of capacitance at low frequencies were attributed to the excess capacitance resulting from the interface states in equilibrium with the ZnO that can follow the alternating current (ac) signal.},
	number = {1–2},
	urldate = {2017-05-30},
	journal = {Journal of Alloys and Compounds},
	author = {Aydogan, S. and Cinar, K. and Asil, H. and Coskun, C. and Turut, A.},
	month = may,
	year = {2009},
	keywords = {Barrier height, n-zno, Schottky contact, Series resistance},
	pages = {913--918},
	file = {Aydoğan et al_2009_Electrical characterization of Au-n-ZnO Schottky contacts on n-Si.pdf:/Users/eazhar/Library/Application Support/Firefox/Profiles/3lz1vxss.default-1491192058467/zotero/storage/GS8EU6SF/Aydoğan et al_2009_Electrical characterization of Au-n-ZnO Schottky contacts on n-Si.pdf:application/pdf;ScienceDirect Snapshot:/Users/eazhar/Library/Application Support/Firefox/Profiles/3lz1vxss.default-1491192058467/zotero/storage/68QI7QKU/S0925838808015971.html:text/html}
