@article{lien_atmospheric_2013,
	title = {Atmospheric pressure plasma jet annealed {ZnO} films for {MgZnO}/{ZnO} heterojunctions},
	volume = {46},
	issn = {0022-3727},
	doi = {10.1088/0022-3727/46/7/075202},
	url = {http://doi.org/10.1088/0022-3727/46/7/075202},
	abstract = {Rf-sputtered ZnO films, annealed by atmospheric pressure plasma jet (APPJ), are characterized and used for MgZnO/ZnO heterostructures. The highly reactive N 2 plasma generated by APPJ allows much shorter treatment time compared with conventional thermal anneals. The APPJ treatment can increase the crystallinity of ZnO films and release the compressive residue stresses, verified by XRD and UV–Vis transmission measurements. In our previous studies, we demonstrate that thermal anneal is the critical step for the formation of two-dimensional electron gases in defective rf-sputtered MgZnO/ZnO heterostructures. This paper reports the experimental results that APPJ treatments can be used for the same purpose with a much shorter processing time. A thirty-second APPJ anneal on ZnO can be used to replace 400 °C × 30 min furnace-anneal to promote the formation of 2DEGs in MgZnO/ZnO heterostructure. The ultra-short processing time is attributed to the synergy of plasma reactivity and temperature of APPJ.},
