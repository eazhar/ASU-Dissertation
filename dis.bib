@article{najbar_studies_2004,
	series = {Air {Pollution} {Abatement} {Symposium}},
	title = {Studies of processes occurring during alkoxide derived {V}–{O}–{W} unsupported catalyst formation},
	volume = {90},
	issn = {0920-5861},
	doi = {10.1016/j.cattod.2004.04.013},
	url = {http://doi.org/10.1016/j.cattod.2004.04.013},
	abstract = {The aim of this paper is to investigate the processes occurring during V–O–W unsupported catalyst formation in order to gain the knowledge necessary to design supported catalysts. Sol–gel synthesis of V–W hydroxo-oxide hydrate (W:V=9:1) from tungsten and vanadyl isopropoxides is described. The hydrate structure is shown to be related to WO3·0.33H2O. The role of vanadium in the formation of this structure is discussed. Hydrate dehydration is shown to result in the formation of V–W hydroxo-oxide isostructural with hexagonal WO3. Further heating of hydroxy-oxide causes a loss of hydroxy groups accompanied by the formation of V–W oxide bronze isostructural with tetragonal WO3. Annealing the bronze in an air atmosphere results in the removal of vanadium from the bronze crystallites as a results of its surface segregation. The formation of the surface vanadia-like species on WO3-related crystallites is found to be a result of vanadium segregation. Increasing the annealing temperature causes recrystallisation of the surface species. The synthesis of a V–W/TiO2 oxide catalyst via a mixed oxide formation in the presence of titania sols followed, by surface vanadium segregation in an air containing atmosphere, is proposed. The application of the knowledge gained during our investigation of the formation of the unsupported V–O–W catalyst to the discussion of the morphology of the V–O–W/Ti(Sn)O2 (rutile) catalyst is also demonstrated.},
	number = {1–2},
	urldate = {2016-07-13},
	journal = {Catalysis Today},
	author = {Najbar, M. and Mizukami, F. and Kornelak, P. and Wesełucha-Birczyńska, A. and Borzcka-Prokop, B. and Bielańska, E. and Białas, A. and Banaś, J. and Su, D.},
	month = jun,
	year = {2004},
	keywords = {Sol–gel synthesis, Unsupported V–O–W catalyst, Vanadia-like surface species, V–W hydroxo-oxide hydrate, V–W oxide bronze},
	pages = {93--102},
	file = {Najbar et al-2004-Studies of processes occurring during alkoxide derived V–O–W unsupported.pdf:/Users/eazhar/Dropbox/Research/_Literature Backup//Electrochromic/W-TiO2/Najbar et al-2004-Studies of processes occurring during alkoxide derived V–O–W unsupported.pdf:application/pdf;ScienceDirect Snapshot:/Users/eazhar/Library/Application Support/Firefox/Profiles/vv3y72ot.default-1470452355141/zotero/storage/KARGNVBZ/S0920586104001312.html:text/html}
}

@article{chen_printed_2015,
	title = {Printed {Multicolor} {High}-{Contrast} {Electrochromic} {Devices}},
	volume = {7},
	issn = {1944-8244},
	doi = {10.1021/acsami.5b08061},
	url = {http://doi.org/10.1021/acsami.5b08061},
	abstract = {In this study, electrochemical responses of inkjet-printed multicolored electrochromic devices (ECD) were studied to evaluate the feasibility of presenting multiple colors in one ECD. Metallo-supramolecular polymers (MEPE) solutions with two primary colors were inkjet-printed on flexible electrodes. By digitally controlling print dosages of each species, the colors of the printed EC thin film patterns can be adjusted directly without premixing or synthesizing new materials. The printed EC thin films were then laminated with a solid transparent thin film electrolyte and a transparent conductive thin film to form an ECD. After applying a dc voltage, the printed ECDs exhibited great contrast with a transmittance change (ΔT) of 40.1\% and a high coloration efficiency of 445 cm2 C–1 within a short darkening time of 2 s. The flexible ECDs also showed the same darkening time of 2 s and still had a high ΔT of 30.1\% under bending condition. This study demonstrated the feasibility to fabricate display devices with different color setups by an all-solution process and can be further extended to other types of displays.},
	number = {45},
	urldate = {2016-07-02},
	journal = {ACS Applied Materials \& Interfaces},
	author = {Chen, Bo-Han and Kao, Sheng-Yuan and Hu, Chih-Wei and Higuchi, Masayoshi and Ho, Kuo-Chuan and Liao, Ying-Chih},
	month = nov,
	year = {2015},
	pages = {25069--25076},
	file = {ACS Full Text Snapshot:/Users/eazhar/Library/Application Support/Firefox/Profiles/vv3y72ot.default-1470452355141/zotero/storage/34MUVDZI/acsami.html:text/html;Chen et al-2015-Printed Multicolor High-Contrast Electrochromic Devices.pdf:/Users/eazhar/Dropbox/Research/_Literature Backup//Electrochromic/Inkjet/2016/Chen et al-2015-Printed Multicolor High-Contrast Electrochromic Devices.pdf:application/pdf}
}

