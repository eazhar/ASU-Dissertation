	keywords = {electrochemical properties, mesoporous materials, oxides, self-assembly, sol–gel processes},
	pages = {1203--1211},
	file = {Full Text PDF:C\:\\Users\\eazharViz\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\7rz610p7.default\\zotero\\storage\\P5CGC6EP\\Brezesinski et al. - 2006 - Highly Crystalline WO3 Thin Films with Ordered 3D .pdf:application/pdf;Snapshot:C\:\\Users\\eazharViz\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\7rz610p7.default\\zotero\\storage\\AZ5K8G8T\\abstract\;jsessionid=E46934FBA4F99620FF2129F931242053.html:text/html}
}


