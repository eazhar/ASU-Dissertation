	file = {Snapshot:/Users/eazhar/Library/Application Support/Firefox/Profiles/vv3y72ot.default-1470452355141/zotero/storage/86UC7ECM/c3tb20639d.html:text/html;Tarabella et al - 2013 - Irreversible evolution of eumelanin redox states detected by an organic.pdf:/Users/eazhar/Dropbox/Research/_Literature Backup//ZnOPEDOT Memristor/Hysteresis/Eumelanin Hysteresis/Tarabella et al - 2013 - Irreversible evolution of eumelanin redox states detected by an organic.pdf:application/pdf}
}

@article{ambrico_commercial_2015,
	title = {From commercial tyrosine polymers to a tailored polydopamine platform: concepts, issues and challenges en route to melanin-based bioelectronics},
	volume = {3},
	issn = {2050-7534},
	shorttitle = {From commercial tyrosine polymers to a tailored polydopamine platform},
	doi = {10.1039/C5TC00570A},
	url = {http://doi.org/10.1039/C5TC00570A},
	abstract = {Over the past decade synthetic melanins, melanin-like polymers and melanin-based copolymers have been the focus of growing attention as soft biocompatible functional materials for engineering high performance, low cost optoelectronic devices, such as memory devices, light emitting diodes and field effect transistors. The unique combination of physicochemical properties of melanins, such as broad band absorption in the UV-visible range, intrinsic free radical character, water-dependent hybrid ionic–electronic conductor behaviour and excellent biocompatibility, have inspired use of melanic polymers as valuable functional materials for organic bioelectronics. However, several gaps and issues still hinder rapid progress of melanin-based organic electronics and bioelectronics, including in particular the limited contribution of electronic conductivity and current decay with time under biasing. The aim of this paper is to provide an overview of the structural and optoelectronic properties of melanins and to bring to focus current gaps and challenges in the development of melanin-based materials for bioelectronics. Starting from commercial samples, the paper surveys different melanin-type materials with special emphasis on the potential of polydopamine (pDA), a highly adhesive mussel-inspired melanin-type platform, for incorporation in optoelectronic devices. Simple chemical tailoring procedures for engineering pDA-based n-type polymers and photoresponsive materials for photocapacitive sensors are eventually illustrated.},
	language = {en},
	number = {25},
	urldate = {2016-11-12},
	author = {Ambrico, Marianna and Ambrico, Paolo F. and Ligonzo, Teresa and Cardone, Antonio and Cicco, Stefania R. and d'Ischia, Marco and Farinola, Gianluca M.},
	month = jun,
	year = {2015},
	pages = {6413--6423},
	file = {Ambrico et al - 2015 - From commercial tyrosine polymers to a tailored polydopamine platform.pdf:/Users/eazhar/Dropbox/Research/_Literature Backup//ZnOPEDOT Memristor/Hysteresis/Eumelanin Hysteresis/Ambrico et al - 2015 - From commercial tyrosine polymers to a tailored polydopamine platform.pdf:application/pdf;Snapshot:/Users/eazhar/Library/Application Support/Firefox/Profiles/vv3y72ot.default-1470452355141/zotero/storage/SGZGUHQZ/C5TC00570A.html:text/html}
}

