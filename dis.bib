	keywords = {Doping, electrical conductivity, Electron paramagnetic resonance, Ion-exchange, Poly(3,4-ethylenedioxythiophene), Raman},
	pages = {80--88},
	file = {Chiu et al - 2005 - Spectroscopic and conductivity studies of doping in chemically synthesized.pdf:/Users/eazhar/Dropbox/Research/_Literature Backup//ZnOPEDOT Memristor/PEDOT Properties/XPS/Chiu et al - 2005 - Spectroscopic and conductivity studies of doping in chemically synthesized.pdf:application/pdf;ScienceDirect Snapshot:/Users/eazhar/Library/Application Support/Firefox/Profiles/vv3y72ot.default-1470452355141/zotero/storage/5U2R9MF7/S0379677905007289.html:text/html}
}

