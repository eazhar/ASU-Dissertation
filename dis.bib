	keywords = {activation energy, additive blends, aromatic polyesters, c-13, crystal-structure, dicarboxylate), molecular-motion, nmr, nuclear-magnetic-resonance, polyesters, polyethylene   terephthalate, relaxation, secondary relaxation processes, transitions},
	pages = {2791--2796}
}

